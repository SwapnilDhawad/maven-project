pipeline 
{
agent any 
  stages 
  {
       stage('mvn compile')
       {
         steps{
          withMaven(jdk: 'JDKAWS', maven: 'MavenAWS'){
		  sh 'mvn compile'}
         }
       }
	   stage('test'){
	   steps{
	    withMaven(jdk: 'JDKAWS', maven: 'MavenAWS'){
		  sh 'mvn test'}
	   }
	   
	   }
	   
	    stage('maven build'){
	   steps{
	    withMaven(jdk: 'JDKAWS', maven: 'MavenAWS'){
		  sh 'mvn package'}
	   }
	   
	   }
	   
	   stage('maven install'){
	   steps{
	    withMaven(jdk: 'JDKAWS', maven: 'MavenAWS'){
		  sh 'mvn install'}
	   }
	   
	   }
      
	   
 }
 
 }

