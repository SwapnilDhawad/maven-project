pipeline{
    agent any 
    stages{
        stage('Build Application'){
            steps{
                sh 'mvn clean package'
            }
        post{
            success{
                echo 'Package the application'
                archiveArtifacts artifacts:'**/*.war'
            }
        }
    }
    stage ('Deploy to the Staging env'){
           
             steps{
                build job: 'Deploy_Staging_Server'
 
            }

     }
 } 
}
